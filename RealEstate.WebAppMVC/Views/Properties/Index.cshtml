                                        <h2><a href="@Url.Action("Details","Properties",new {id = sliderProperty.PropertyId})">@sliderProperty.FullAddress</a></h2>
                                        <p>@sliderProperty.AdditionalDescription</p>
                                    </div>
                                </div>
                            </div>
                        </div>
